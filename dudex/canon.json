{
  "canon_version": "1.0",
  "description": "a custom react + typescript todo app with tailwind css and a predictive ai assistant that generates a daily to-do list based on user patterns.",
  "features": [
    "add/edit/delete tasks",
    "filter tasks (all/active/completed)",
    "localstorage persistence",
    "mark tasks complete",
    "predictive ai assistant (client-side)"
  ],
  "pages": [
    "analytics",
    "home",
    "settings"
  ],
  "project": {
    "framework": "react",
    "language": "typescript",
    "name": "task tracker",
    "type": "web_app"
  },
  "requirements": {
    "accessibility": true,
    "analytics": false,
    "performance": "standard",
    "responsive": true,
    "seo": false
  },
  "styling": {
    "color_scheme": "custom",
    "framework": "tailwind"
  }
}